  echo -e "       |  \ /     \ /  |" | lolcat
  echo -e "       | \  |     |  / |" | lolcat
  echo -e "       | \ _\_/^\_/_ / |" | lolcat
  echo -e "       |    --\//--    |" | lolcat
  echo -e "        \_\ ELEMENT / _/" | lolcat
  echo -e "          \__  |  __/" | lolcat
  echo -e "             \ _ /" | lolcat
  echo -e "            _/   \_  \033[92m  -->> SCRIPT:\033[93m Element Manager " | lolcat
  echo -e "           / _/|\_ \ \033[92m  -->> DEVELOPER:\033[93m @Near365 " | lolcat
  echo -e "           /   |  \  " | lolcat
  echo -e "             / v \   " | lolcat
  echo ""
  msgi -ama "  [ --- \033[1;97m ๐ฒ ยกSimple, eficaz y potente! ๐ฒ\033[1;33m --- ]"
  echo -e "\033[1;32mโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโ"
  msgi -bar2
  echo -ne "\033[1;96m          >>> INTRODUZCA LA KEY ABAJO <<<\n\033[1;31m        " && read LICENSE_KEY

  # Realizar la verificaciรณn de la licencia
  RESPONSE=$(verify_license_test "$LICENSE_KEY")
  # Analizar la respuesta del servidor
  if [[ $RESPONSE == *"Licencia vรกlida"* ]]; then
      #success_msg "La licencia es vรกlida. Continuando con la instalaciรณn..."
      instalador_final
  else
      error_fun
  fi

}
